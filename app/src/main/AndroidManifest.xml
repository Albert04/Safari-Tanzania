<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.safari">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_safari"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_safari_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Safari">
        <activity android:name=".dashboard.ui.NgoroWebViewActivity"></activity>
        <activity android:name=".dashboard.WebViewActivity"></activity>
        <activity android:name=".dashboard.ImagesActivity" />
        <activity
            android:name=".dashboard.HomeActivity"
            android:label="@string/title_activity_home" />
        <activity android:name=".NewregisterActivity" />
        <activity android:name=".dashboard.BookingActivity" />
        <activity android:name=".dashboard.CultureActivity" />
        <activity android:name=".dashboard.KilimanjaroActivity" />
        <activity android:name=".dashboard.MuseumActivity" />
        <activity android:name=".dashboard.woldHeritegeActivity" />
        <activity android:name=".dashboard.ConsevartionActivity" />
        <activity android:name=".dashboard.RestaurantActivity" />
        <activity android:name=".dashboard.HotelActivity" />
        <activity android:name=".dashboard.NationparkActivity" />
        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".dashboard.DashboardActivity" />
        <activity android:name=".dashboard.RegisterActivity" />
        <activity android:name=".MainActivity" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>